<div class="historial">
    <h3>Historial personas</h3>
    <% if (personas.length > 0) { %>
        <table class="table table-dark table-hover">
            <thead>
                <tr><th>Nombre</th><th>Apellido</th><th>DNI</th></tr>
            </thead>
            <tbody>
                <% for( persona of  personas) { %>
                    <tr>
                        <td><%= persona.name %> </td>
                        <td><%= persona.lastname %> </td>
                        <td><%= persona.dni %> </td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    <% } else { %>
        <p class="alert alert-danger">No encontramos Data, ingresalos!!</p>
    <% } %>
</div>
